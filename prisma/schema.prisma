generator db {
    provider = "prisma-client-py"
    interface = "sync"
}
datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model Pertanyaan {
  id        Int      @id @default(autoincrement())
  pertanyaan String
  createdAt DateTime @default(now())
}

model Jawaban {
  id        Int      @id @default(autoincrement())
  jawaban   String
  createdAt DateTime @default(now())
}

